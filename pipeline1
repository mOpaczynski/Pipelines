pipeline {
  agent none
  stages {
    stage('Build') {
      steps {
        echo 'Building...'
      }
    }
    stage('Test') {
      steps {
        echo 'Testing...'
      }
    }
    stage('Deploy') {
      steps {
        echo 'Deploying...'
      }
	}
    stage('Second deploy') {
      steps {
        echo 'Deploying a second time...'
      }
    }
	stage('This is a pull request test') {
      steps {
        echo 'Deploying the pull request...'
      }
    }
	stage('Another branch') {
      steps {
        echo 'Deploying the pull request...'
      }
    }
  }
}